{
	"name": "Read Line",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Read the English paragraph line by line",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	//"permissions": ["tabs", "http://*/*", "https://*/*"]
	"permissions": ["tabs", "http://*/*", "https://*/*", "file:///*/*"]
}